{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "items": {
      "matchCondition": {
        "route": "/api/v1/items"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/Items"
    },
    "ui": {
      "matchCondition": {
        "route": "/"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/UI"
    }
  }
}
