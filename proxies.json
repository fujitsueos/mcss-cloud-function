{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "Items": {
      "matchCondition": {
        "route": "/api/items",
        "methods": ["GET", "POST"]
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/Items"
    },
    "UI": {
      "matchCondition": {
        "route": "/ui",
        "methods": ["GET"]
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/UI"
    }
  }
}
